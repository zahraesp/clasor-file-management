{
  "name": "cls-file-management",
  "private": false,
  "version": "0.8.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:css": "tailwindcss -o dist/style.css --minify"
  },
  "files": [
    "src",
    "dist"
